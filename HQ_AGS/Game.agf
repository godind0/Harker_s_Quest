                <Script><![CDATA[// Dialog script file
@S  // Dialog startup entry point
HARKER: Good day, Mr. Hawkins
HAWKINS: Jonathan.
return
@1
HAWKINS: I am not great. My joints have been hurting more than usual.
HAWKINS: My physician says I should not travel...
HAWKINS: But I have this estate sale coming up.
HARKER: Right
option-off-forever 1
  if(Game.DoOnceOnly("AskHawkinsHowHeIsDoing")){
    GiveScore(1);
  }
return
@2
  if(!player.HasInventory(iNotebook))
  {
    cHawkins.Say("What? No notebook?");
    cHawkins.Say("Go get your notebook Harker!");
  } else if(!player.HasInventory(iLegalPapers))
  {
    cHawkins.Say("Where are they? Go get them!");
  } else {
    cHawkins.Say("Ah yes! Give them here!");
    player.LoseInventory(iLegalPapers);
    if(iLegalPapers.Name != "Legal dossier")
    {
      cHawkins.Say("You could not put them together in a folder?");
      cHawkins.Say("Everything is in order, at least. Good");
    } else {
      cHawkins.Say("Complete and neat, well done Harker");
      if(Game.DoOnceOnly("HandedInALegalDossier")){
        GiveScore(1);
      }
    }
    dHawkins1.SetOptionState(2,eOptionOffForever);
    dHawkins1.SetOptionState(3,eOptionOn);
    dHawkins1.SetOptionState(5,eOptionOn);
    //debugDisplay(String.Format("Found score: %d[Current time: %s[Current day: %d",game.score,whatTimeIsIt(false),currentDay));
    if(isItBeforeThisTime(4,0,0,true,1)) { //Before 4PM
      if(game.score < 10)
      {
        //debugDisplay("Normal");
        cHawkins.Say("I have a new assignment for you");
        cHawkins.Say("We have another client interested in a new property");
        CallRoomScript(2);
      } else {
        //debugDisplay("Final");
        cHawkins.Say("I have a new assignment for you");
        cHawkins.Say("We have another client interested in a new property");
        CallRoomScript(3);
      }
    } else {
      cHawkins.Say("That is all for today, Harker");
      cHawkins.Say("Dismissed");
      dHawkins1.SetOptionState(2,eOptionOff);
      dHawkins1.SetOptionState(3,eOptionOff);
      dHawkins1.SetOptionState(4,eOptionOff);
      dHawkins1.SetOptionState(5,eOptionOff);
    }
  }
return
@3
HAWKINS: Ah yes! What property are you proposing?
  lblSimpleTextbox.Text = "Suggested property code";
  tbSimpleTextbox.Text = "";
  open_gui(gSimpleTextbox);
stop
@4
return
@5
HAWKINS: I told you to note it down.
  CallRoomScript(5);
return
@6
HAWKINS: Sure. Let's get back to work.
stop
]]></Script>
